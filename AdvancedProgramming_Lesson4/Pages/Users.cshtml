@page
@model AdvancedProgramming_Lesson4.Pages.UsersModel
@{
	ViewData["Title"] = "Users";
}
<h1>Users</h1>
<table class="table">
	<thead>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.Users[0].UserName)
			</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model.Users)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.UserName)
				</td>
			</tr>
		}
	</tbody>
</table>
